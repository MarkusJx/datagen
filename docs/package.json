{
  "name": "@datagen/docs",
  "version": "0.1.0",
  "private": true,
  "description": "Docs for datagen",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run build:wasm && npm run copy:schema",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "check:markdown": "find . -type f -name '*.md*' ! -path './node_modules/*' -exec markdown-link-check {} \\;",
    "build:wasm": "cd ../crates/datagen-wasm && wasm-pack build -t web -d web-pkg --release --features generate",
    "copy:schema": "cpy --flat ../packages/types/dist/schema.json ./public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkusJx/datagen.git"
  },
  "author": "MarkusJx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkusJx/datagen/issues"
  },
  "homepage": "https://github.com/MarkusJx/datagen#readme",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.14.13",
    "@mui/lab": "^5.0.0-alpha.148",
    "@mui/material": "^5.14.13",
    "@octokit/rest": "^20.0.2",
    "arch": "^2.2.0",
    "datagen-rs-types": "file:../packages/types",
    "datagen-wasm": "file:../crates/datagen-wasm/web-pkg",
    "next": "^13.5.4",
    "nextra": "^2.13.2",
    "nextra-theme-docs": "^2.13.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "semver": "^7.5.4",
    "threads": "^1.7.0",
    "ua-parser-js": "^1.0.36"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/react-dom": "^18.2.13",
    "@types/ua-parser-js": "^0.7.37",
    "cpy-cli": "^5.0.0",
    "markdown-link-check": "^3.11.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "wasm-pack": "^0.12.1"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}

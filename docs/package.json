{
  "name": "@datagen/docs",
  "version": "0.0.1",
  "private": true,
  "description": "Nextra docs template",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "check:markdown": "find . -type f -name '*.md*' ! -path './node_modules/*' -exec markdown-link-check {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkusJx/datagen.git"
  },
  "author": "MarkusJx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkusJx/datagen/issues"
  },
  "homepage": "https://github.com/MarkusJx/datagen#readme",
  "dependencies": {
    "next": "^13.0.6",
    "nextra": "latest",
    "nextra-theme-docs": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.10",
    "markdown-link-check": "^3.11.2",
    "prettier": "^3.0.3",
    "typescript": "^4.9.3"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}

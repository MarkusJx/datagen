{
    "name": "datagen-node",
    "version": "0.0.0",
    "main": "index.js",
    "types": "index.d.ts",
    "napi": {
        "name": "datagen-node",
        "triples": {
            "additional": [
                "aarch64-apple-darwin",
                "i686-pc-windows-msvc"
            ]
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@napi-rs/cli": "^2.16.2",
        "ava": "^5.1.1",
        "json-schema-to-typescript": "^13.1.1",
        "prettier": "^3.0.3",
        "tsx": "^3.12.9"
    },
    "ava": {
        "timeout": "3m"
    },
    "engines": {
        "node": ">= 10"
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "scripts": {
        "artifacts": "napi artifacts",
        "build": "napi build --platform --release",
        "build:debug": "napi build --platform",
        "postbuild": "tsx ts-src/scripts/postbuild.ts",
        "prepublishOnly": "napi prepublish -t npm",
        "test": "ava",
        "universal": "napi universal",
        "version": "napi version",
        "format": "prettier --write ."
    }
}
